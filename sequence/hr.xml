<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <!-- Leave Account: Leave Account -->
        <record id="leave_account_sequence" model="ir.sequence">
            <field name="name">Leave Account</field>
            <field name="code">leave.account</field>
            <field name="prefix">LAC/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Leave Account: Leave Journal Entry -->
        <record id="leave_journal_entry_sequence" model="ir.sequence">
            <field name="name">Leave Journal Entry</field>
            <field name="code">leave.journal.entry</field>
            <field name="prefix">LJE/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Leave Account : Leave Journal Item -->
        <record id="leave_journal_item_sequence" model="ir.sequence">
            <field name="name">Leave Journal Item</field>
            <field name="code">leave.journal.item</field>
            <field name="prefix">LJI/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Leave Account : Leave Voucher -->
        <record id="leave_voucher_sequence" model="ir.sequence">
            <field name="name">Leave Voucher</field>
